{
  "ignorePatterns": [
    {
      "pattern": "^https://example\\.com"
    },
    {
      "pattern": "^http://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://your-website.com/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://api.github.com"],
      "headers": {
        "Authorization": "Bearer YOUR_GITHUB_TOKEN"
      }
    }
  ],
  "timeout": 10,
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}